<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.inspiron.companywiki.Lgsignup"
    android:background="@drawable/lgg_3">
    <RelativeLayout
        android:layout_width="395dp"
        android:layout_height="587dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Sign UP"
            android:onClick="bfaalg"
            android:background="#ffffff"
            android:layout_above="@+id/button2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="13dp"
            android:id="@+id/button7" />
        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="BACK"
            android:background="#ffffff"
            android:onClick="llllg"
            tools:layout_editor_absoluteY="379dp"
            tools:layout_editor_absoluteX="-36dp"
            android:layout_above="@+id/button"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="14dp"
            android:id="@+id/button2" />
        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Home Screen"
            android:onClick="kkklg"
            android:background="#ffffff"
            tools:layout_editor_absoluteY="443dp"
            tools:layout_editor_absoluteX="0dp"
            android:layout_marginBottom="97dp"
            android:id="@+id/button"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter Email ID"
            android:background="#000000"
            android:textColor="#FFFFFFFF"
            android:textSize="20dp"
            android:layout_below="@+id/textView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="10dp" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:text="ENTER YOUR EMAIL HERE"
            android:background="#FFFFFFFF"
            android:textSize="30dp"
            android:textColor="#000000"
            android:inputType="textEmailAddress"
            android:layout_below="@+id/button6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:id="@+id/button8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter Password"
            android:background="#000000"
            android:textColor="#FFFFFFFF"
            android:textSize="20dp"
            android:layout_below="@+id/editText3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="15dp" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textSize="30dp"
            android:background="#FFFFFFFF"
            android:inputType="textPassword"
            android:layout_below="@+id/button8"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter NAME"
            android:background="#000000"
            android:textColor="#FFFFFFFF"
            android:textSize="20dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ENTER YOUR NAME HERE"
            android:background="#FFFFFFFF"
            android:textSize="30dp"
            android:textColor="#000000"
            android:inputType="textEmailAddress"
            android:layout_below="@+id/button9"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
