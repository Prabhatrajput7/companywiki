<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.inspiron.companywiki.Applec"
    android:background="@drawable/aaaa">
    <RelativeLayout
        android:layout_width="395dp"
        android:layout_height="587dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:onClick="opepx"
            android:text="Back"
            android:background="#FFFFFFFF"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="248dp"
            android:id="@+id/button18" />
        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Home Screen"
            android:onClick="andp"
            android:background="#FFFFFFFF"
            android:layout_alignTop="@+id/button18"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="59dp" />

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button18"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="23dp"
            android:layout_marginLeft="126dp"
            android:layout_marginStart="126dp"
            android:text="Click here to locate"
            android:textColor="#FFFFFFFF"
            android:textSize="20dp"
            android:onClick="elp"
            android:background="#000000"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button19"
            android:layout_alignLeft="@+id/button19"
            android:layout_alignStart="@+id/button19"
            android:layout_marginBottom="42dp"
            android:text="Click here to call"
            android:textColor="#FFFFFFFF"
            android:textSize="20dp"
            android:background="#000000"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Button
            android:id="@+id/button21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Click here to EMAIL"
            android:textColor="#FFFFFFFF"
            android:textSize="20dp"
            android:background="#000000"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/button20"
            android:layout_alignStart="@+id/button20"
            android:layout_marginTop="30dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />


    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
